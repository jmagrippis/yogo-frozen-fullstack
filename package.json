{
  "name": "yogo-frozen-fullstack",
  "version": "0.1.0",
  "description": "A modest frontend for the Yogo Frozen website, aimed to be served via Horizon.",
  "main": "index.jsx",
  "scripts": {
    "start": "webpack-dev-server",
    "test": "standard && env NODE_PATH=$NODE_PATH:$PWD/src mocha --compilers js:babel-core/register --require ./test/.setup.js 'test/**/*.@(js|jsx)'",
    "test:watch": "npm run test -- --watch --watch-extensions jsx",
    "build": "NODE_ENV=production webpack -p"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jmagrippis/yogo-frozen-fullstack"
  },
  "dependencies": {
    "aphrodite": "^0.3.0",
    "immutable": "^3.0.0",
    "lodash": "^4.0.0",
    "material-ui": "^0.15.0",
    "react": "^15.0.0",
    "react-addons-pure-render-mixin": "^15.0.0",
    "react-dom": "^15.0.0",
    "react-google-maps": "^4.0.0",
    "react-redux": "^4.0.0",
    "react-tap-event-plugin": "^1.0.0",
    "redux": "^3.0.0",
    "redux-thunk": "^2.0.0"
  },
  "devDependencies": {
    "babel-core": "^6.0.0",
    "babel-eslint": "^6.0.0",
    "babel-loader": "^6.0.0",
    "babel-polyfill": "^6.0.0",
    "babel-preset-es2015": "^6.0.0",
    "babel-preset-react": "^6.0.0",
    "babel-preset-stage-0": "^6.0.0",
    "babel-register": "^6.0.0",
    "chai": "^3.0.0",
    "chai-immutable": "^1.0.0",
    "css-loader": "^0.23.0",
    "enzyme": "^2.0.0",
    "isomorphic-fetch": "^2.0.0",
    "jsdom": "^9.0.0",
    "mocha": "^2.0.0",
    "normalize.css": "^4.0.0",
    "react-addons-test-utils": "^15.0.0",
    "react-hot-loader": "^3.0.0-0",
    "redux-devtools": "^3.0.0",
    "redux-devtools-dock-monitor": "^1.0.0",
    "redux-devtools-log-monitor": "^1.0.0",
    "sinon": "^1.0.0",
    "standard": "^7.0.0",
    "style-loader": "^0.13.0",
    "webpack": "^1.0.0",
    "webpack-dev-server": "^1.0.0"
  },
  "babel": {
    "presets": [
      "es2015",
      "stage-0",
      "react"
    ],
    "plugins": [
      "react-hot-loader/babel"
    ]
  },
  "standard": {
    "ignore": [
      "blueprints/**",
      "dist/**"
    ],
    "globals": [
      "describe",
      "it",
      "localStorage"
    ],
    "parser": "babel-eslint"
  },
  "keywords": [
    "react",
    "redux",
    "yoghurt",
    "froyo"
  ],
  "license": "MIT",
  "private": true
}
